@page
@using StokTakip.Localization
@using StokTakip.Web.Pages.Products
@using Microsoft.Extensions.Localization
@using Volo.Abp.AspNetCore.Mvc.UI.Bootstrap.TagHelpers.Modal
@inject IStringLocalizer<StokTakipResource> L
@model StokTakip.Web.Pages.Products.SaleModalModel
@{
    Layout = null;
}


<abp-dynamic-form abp-model="sale" asp-page="/Products/SaleModal">
    <abp-modal centered="true" size="Large">
        <abp-modal-header title="@L["Sale"].Value"></abp-modal-header>
        <abp-modal-body>
            <abp-modal-body>
                <abp-form-content />
            </abp-modal-body>
        </abp-modal-body>
        <abp-modal-footer buttons="@(AbpModalButtons.Cancel|AbpModalButtons.Save)"></abp-modal-footer>
    </abp-modal>
</abp-dynamic-form>