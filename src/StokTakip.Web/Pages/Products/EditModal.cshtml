@page
@using StokTakip.Localization
@using StokTakip.Web.Pages.Products
@using Microsoft.Extensions.Localization
@using Volo.Abp.AspNetCore.Mvc.UI.Bootstrap.TagHelpers.Modal
@inject IStringLocalizer<StokTakipResource> L
@model StokTakip.Web.Pages.Products.EditModalModel
@{
    Layout = null;
}
<script type="text/javascript" src="~/Pages/Products/CreateModal.js"></script>
<script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>


<abp-dynamic-form abp-model="productInput" asp-page="/Products/EditModal">
    <abp-modal centered="true" size="ExtraLarge">
        <abp-modal-header title="@L["EditProduct"].Value"></abp-modal-header>
        <abp-modal-body>
            <abp-modal-body>
                <abp-form-content />
                <abp-input asp-for="UploadFileDtos.Name"></abp-input>

                <abp-input asp-for="UploadFileDtos.File"></abp-input>
            </abp-modal-body>
        </abp-modal-body>
        <abp-modal-footer buttons="@(AbpModalButtons.Cancel|AbpModalButtons.Save)"></abp-modal-footer>
    </abp-modal>
</abp-dynamic-form>