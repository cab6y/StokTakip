@page
@using StokTakip.Localization
@using StokTakip.Web.Pages.Products
@using Microsoft.Extensions.Localization
@using Volo.Abp.AspNetCore.Mvc.UI.Bootstrap.TagHelpers.Modal
@inject IStringLocalizer<StokTakipResource> L
@model StokTakip.Web.Pages.Products.SizeCreateModalModel
@{
    Layout = null;
}
<script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>


<abp-dynamic-form abp-model="create" asp-page="/Products/SizeCreateModal">
    <abp-modal centered="true" size="ExtraLarge">
        <abp-modal-header title="@L["NewProductSize"].Value"></abp-modal-header>
        <abp-modal-body>
            <abp-modal-body>
                <abp-form-content />
            </abp-modal-body>
        </abp-modal-body>
        <abp-modal-footer buttons="@(AbpModalButtons.Cancel|AbpModalButtons.Save)"></abp-modal-footer>
    </abp-modal>
</abp-dynamic-form>